{
  "name": "@nsoft/vuetify-uploader",
  "version": "1.0.15-beta.3",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vuetify-uploader src/index.js",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "coverage": "npm test && codecov",
    "prepublish": "npm run build"
  },
  "license": "MIT",
  "author": "Chameleon Team <chameleon@nsoft.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/nsftx/vuetify-uploader"
  },
  "main": "dist/vuetify-uploader.umd.js",
  "dependencies": {
    "axios": "^0.19.0",
    "pretty-bytes": "^5.2.0",
    "vue": "^2.6.10",
    "vuetify": "^2.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-service": "^3.9.3",
    "@vue/eslint-config-airbnb": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.28",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.5.0",
    "eslint-plugin-jest": "^22.13.6",
    "eslint-plugin-vuetify": "^1.0.0-beta.3",
    "sass": "^1.22.7",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-vuetify": "git+https://github.com/vuetifyjs/vue-cli-plugin-vuetify.git#dev",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "babel": {
    "plugins": [
      [
        "transform-imports",
        {
          "vuetify": {
            "transform": "vuetify/es5/components/${member}",
            "preventFullImport": true
          }
        }
      ]
    ],
    "presets": [
      "@vue/app"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "<rootDir>/(tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx))"
    ]
  }
}
